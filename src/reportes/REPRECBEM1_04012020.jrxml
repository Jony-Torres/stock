	<parameter name="P_COD_SUCURSAL" class="java.lang.String"/>
	<parameter name="P_COD_CLIENTE" class="java.lang.Integer"/>
	<parameter name="P_FECHA_REC_DESDE" class="java.lang.String"/>
	<parameter name="P_FECHA_REC_HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT v.cod_sucursal,
	v.desc_sucursal,
	CONCAT(v.tipo_recibo,'-',v.ser_recibo,'-',v.nro_recibo) grupo_rec,
	v.ser_recibo,
	v.nro_recibo,
	v.fecha_recibo,
	v.fecha_rec,
	v.cod_cliente,
	v.nomb_cliente,
	v.cod_moneda,
	v.desc_moneda,
	SUM(v.importe_total) total_comp
FROM view_recibo_venta v
WHERE (v.cod_sucursal= $P{P_COD_SUCURSAL}  OR $P{P_COD_SUCURSAL}  IS NULL)
       AND (v.cod_cliente= $P{P_COD_CLIENTE}  OR $P{P_COD_CLIENTE} IS NULL)
       AND DATE(v.fecha_rec) BETWEEN   $P{P_FECHA_REC_DESDE}  AND  $P{P_FECHA_REC_HASTA}
GROUP BY v.cod_sucursal,
	v.desc_sucursal,
	CONCAT(v.tipo_recibo,'-',v.ser_recibo,'-',v.nro_recibo),
	v.ser_recibo,
	v.nro_recibo,
	v.fecha_recibo,
	v.fecha_rec,
	v.cod_cliente,
	v.nomb_cliente,
	v.cod_moneda,
	v.desc_moneda
ORDER BY 1,8,6,3,5]]>
	</queryString>
